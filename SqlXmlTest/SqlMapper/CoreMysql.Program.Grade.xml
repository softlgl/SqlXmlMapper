  <select id="GetGrades">
    select * from Grade
    where Deleted=@Deleted
    <if test="Name!=null">
      and Name=@Name
    </if>
    <if test="CreateTime neq null">
      and <![CDATA[CreateTime>=@CreateTime]]>
    </if>
    <if test="Ids != null">
      and Id in
      <each name="Ids" open="(" separator="," close=")" prefix="@"  />
    </if>
    limit 1
  </select>
</insql>